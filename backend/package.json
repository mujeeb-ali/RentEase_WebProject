{
  "name": "rentease-backend",
  "version": "2.0.0",
  "description": "RentEase 2.0 - Backend API Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "seed": "node seed-users.js",
    "build": "pkg . --out-path ./dist",
    "build:win": "pkg . --targets node18-win-x64 --out-path ./dist",
    "build:linux": "pkg . --targets node18-linux-x64 --out-path ./dist",
    "build:mac": "pkg . --targets node18-macos-x64 --out-path ./dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": "server.js",
  "pkg": {
    "assets": [
      "node_modules/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "engines": {
   "node": "24.x",
    "npm": "9.x"     

  },
  "keywords": [
    "real-estate",
    "property",
    "rental"
  ],
  "author": "RentEase Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "pm2": "^5.3.0",
    "socket.io": "^4.5.4",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1"
  }
}
